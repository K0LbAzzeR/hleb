<?php
/**
 * Enter console commands from framework HLEB.
 * All service commands: php console --help
 */

$vendor_dir_name = "vendor";
$path = "$vendor_dir_name/phphleb/framework/console.php";
passthru ("php $path " . ($argv[1] ?? null) . " " . __DIR__ . " " . $vendor_dir_name . " " . implode(" ",  $argv));
